        {
            "testament": "NT",
            "book": "Mark",
            "chapter": "11",
            "textSrc": "GOC",
            "language": "Greek",
            "verseNum": 19,
            "cueNdx": 19,
            "cueTime": 153284,
            "cueText": " καὶ ὅτε ὀψὲ ἐγένετο, ἐξεπορεύετο ἔξω τῆς πόλεως\r\n"
        },

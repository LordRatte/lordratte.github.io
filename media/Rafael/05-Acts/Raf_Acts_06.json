        {
            "testament": "NT",
            "book": "Acts",
            "chapter": "6",
            "textSrc": "GOC",
            "language": "Greek",
            "verseNum": 4,
            "cueNdx": 4,
            "cueTime": 34539,
            "cueText": " ἡμεῖς δὲ τῇ προσευχῇ καὶ τῇ διακονίᾳ τοῦ λόγου προσκαρτερήσομεν. "
        },
        {
            "testament": "NT",
            "book": "Acts",
